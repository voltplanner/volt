{
    "name": "package-based",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "nx": "nx",
        "lint:fix": "nx run client:sdk:generate && nx run-many --t lint --fix && nx format:write --all",
        "prepare": "husky",
        "postinstall": "npm run lint:fix",
        "infra:local:up": "docker-compose --file=packages/devops/local/docker-compose.yaml up -d",
        "infra:local:down": "docker-compose --file=packages/devops/local/docker-compose.yaml down",
        "infra:local:volumes:create": "docker volume create volt-local-volume-postgres",
        "infra:local:volumes:delete": "docker volume rm volt-local-volume-postgres"
    },
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@genql/cli": "^6.3.1",
        "@nestjs/schematics": "^10.0.1",
        "@nestjs/testing": "^10.0.2",
        "@nx/cypress": "18.0.2",
        "@nx/devkit": "18.0.2",
        "@nx/esbuild": "18.0.2",
        "@nx/eslint": "18.0.2",
        "@nx/eslint-plugin": "18.0.2",
        "@nx/jest": "18.0.2",
        "@nx/js": "18.0.2",
        "@nx/linter": "18.0.2",
        "@nx/nest": "18.0.2",
        "@nx/node": "18.0.2",
        "@nx/plugin": "18.0.2",
        "@nx/react": "18.0.2",
        "@nx/webpack": "18.0.2",
        "@nx/workspace": "18.0.2",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
        "@svgr/webpack": "^8.0.1",
        "@swc-node/register": "~1.6.7",
        "@swc/cli": "~0.1.62",
        "@swc/core": "~1.3.85",
        "@testing-library/react": "14.0.0",
        "@tinybudgie/schematics": "^0.1.1",
        "@types/jest": "^29.4.0",
        "@types/node": "18.19.14",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "@typescript-eslint/eslint-plugin": "6.21.0",
        "@typescript-eslint/parser": "6.21.0",
        "babel-jest": "^29.4.1",
        "cypress": "^13.0.0",
        "esbuild": "^0.19.2",
        "eslint": "~8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-cypress": "^2.13.4",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "husky": "^9.0.11",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "jest-environment-node": "^29.4.1",
        "nx": "18.0.2",
        "prettier": "^2.6.2",
        "prisma": "^5.8.1",
        "react-refresh": "^0.10.0",
        "ts-jest": "^29.1.0",
        "ts-node": "10.9.1",
        "typescript": "5.3.3",
